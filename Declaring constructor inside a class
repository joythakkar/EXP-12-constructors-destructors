#include <iostream>
using namespace std;

class Student{
    string name;
    int PRN;
    string branch;
    char divi;
    double fee;
    
    public:
    Student(){
        cout<<"Enter the Name:"<<endl;
        cin>>name;
        cout<<"Enter the PRN:"<<endl;
        cin>>PRN;
        cout<<"Enter the Branch Name:"<<endl;
        cin>>branch;
        cout<<"Enter the Division:"<<endl;
        cin>>divi;
        cout<<"Enter the Fee:"<<endl;
        cin>>fee;
    }
    
    void display(){
        cout<<endl<<name<<"\t"<<PRN<<"\t"<<branch<<"\t"<<divi<<"\t"<<fee;
    }
};

int main() {
    Student s;
    s.display();

    return 0;
}

//OUTPUT:
//Enter the Name:
//Ishika
//Enter the PRN:
//9
//Enter the Branch Name:
//entc
//Enter the Division:
//A
//Enter the Fee:
//1.5

//Ishika	9	entc	A	1.5
